  details = <
    ["en"] = <
      language = <[ISO_639-1::en]>
      purpose = <"BodyWeightMeas model is a measurement model. A measurement model holds a 'question' or a 'test' in the key and holds a numeric value answer in data. In the BodyWeightMeas model, the question is 'what is the subject's body weight?' and the numeric answer will be expressed in kilograms. Calculated body weights (e.g. ideal, goal) will be captured in another model.">
      use = <"Use to represent subject's body weight.">
      keywords = <"">
    >
  >

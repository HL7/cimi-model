  details = <
    ["en"] = <
      language = <[ISO_639-1::en]>
      purpose = <"HeartRateMeas is a measurement model. A measurement model holds a 'question' or a 'test' in the key and holds a numeric value 'answer' in data. For example, in the HeartRateMeas model, the 'question' or the 'test' is 'What is the heart rate measurement?' and the 'answer' in data can be '100 bpm.'  'Heart rate' by definition means 'The speed at which the heart is beating.'">
      use = <"Use to represent subject's heart rate.">
      keywords = <"">
    >
  >

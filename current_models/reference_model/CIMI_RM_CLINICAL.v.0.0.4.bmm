				cardinality = <|>=1|>
				is_mandatory = <True>
			>
		>
	>
	["Annotation"] = <
		documentation = <"Text node with optional attribution">
		name = <"Annotation">
		ancestors = <"Locatable",...>
		properties = <
			["authored"] = (P_BMM_SINGLE_PROPERTY) <
				documentation = <"The attribution for the given comment or note.">

			["accountId"] = (P_BMM_SINGLE_PROPERTY) <
				documentation = <"'Contains the account status.' - HL7 Version 2.8, PV1-41. Note that HL7 does not suggest any values for this code (in User-defined Table 117).">
				name = <"accountId">
				type = <"Identifier">
			>
			["accountStatus"] = (P_BMM_SINGLE_PROPERTY) <
